    <lebewesen:Entwiklung>
        <lebewesen:Berechnung id="bmi" Reihenfolge="99999" einheit="kg">
            <misc:Name>
                <misc:Lokalisirung Sprache="de">Platzhalter</misc:Lokalisirung>
            </misc:Name>
            <lebewesen:Formel>1</lebewesen:Formel>
