>
  <Taktik Id="Angriff" Kosten="2" Belastung="1" Typ="Offensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de"><![CDATA[Angriff]]></misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Ein standard angriff.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
  </Taktik>

  <Taktik Id="Leichter Angriff" Kosten="1" Belastung="0.5" Typ="Offensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Leichter Angriff]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Ein leichter Angriff.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <Erfolg>
        <![CDATA[Wenn eine andere Offensive Taktik dieses Charakters mehr oder gleich viel Schaden am selben Ziel verursacht, wird der Schaden negiert.]]>
      </Erfolg>
    <Mod ModifierType="Malus">
      <ConcreteModValueType Type="Absolute" Value="5" />
    </Mod>
  </Taktik>

  <Taktik Id="Offensiver Angriff" Kosten="2" Belastung="1.5" Typ="Offensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Offensiver Angriff]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Ein Angriff der wenig raum für Verteidigung läßt.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <Mod ModifierType="Bonus">
      <ConcreteModValueType Type="Absolute" Value="5" />
    </Mod>
  </Taktik>

  <Taktik Id="Agressiver Angriff" Kosten="4" Belastung="2" Typ="Offensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Agressiver Angriff]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Ein Angriff der die Verteidigung ignoriert.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <Bedingung>
        <![CDATA[Keine Defensiven Taktiken.]]>
      </Bedingung>
    <Mod ModifierType="Bonus">
      <ConcreteModValueType Type="Percent" Value="100" />
    </Mod>
  </Taktik>

  <Taktik Id="Leichte Verteidigung" Kosten="1" Belastung="0.5" Typ="Defensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Leichte Verteidigung]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Eine leichte Verteidigung.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <Mod ModifierType="Malus">
      <ConcreteModValueType Type="Absolute" Value="5" />
    </Mod>
  </Taktik>

  <Taktik Id="Verteidigung" Kosten="2" Belastung="1" Typ="Defensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Verteidigung]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Eine Standard Verteidigung.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
  </Taktik>

  <Taktik Id="Starke Verteidigung" Kosten="3" Belastung="1.5" Typ="Defensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Starke Verteidigung]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Eine Schwer durchdringbare verteidigung.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <Mod ModifierType="Bonus">
      <ConcreteModValueType Type="Absolute" Value="5" />
    </Mod>
  </Taktik>

  <Taktik Id="Verteidigungshaltung" Kosten="3" Belastung="2" Typ="Defensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Verteidigungshaltung]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Der Charakter konzentriert sich auf seine Verteidigung, auf kosten seiner Angriffe.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <Bedingung>
        <![CDATA[Keine offensiven Taktiken.]]>
      </Bedingung>
    <Mod ModifierType="Bonus">
      <ConcreteModValueType Type="Percent" Value="50" />
    </Mod>
  </Taktik>

  <Taktik Id="Rückzug" Kosten="2" Belastung="2" Typ="Defensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Rückzug]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Ein gezielter Rückzug bei dem sich der Charakter aus dem Nahkampf lößt, ohne sich Preis zu geben]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <Erfolg>
        <![CDATA[Der Charakter lößt sich aus dem Nahkampf.]]>
      </Erfolg>
  </Taktik>

  <Taktik Id="Flucht" Kosten="3" Belastung="1" Typ="Neutral">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Flucht]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Der Charakter entfernt sich aus dem Nahkampf ohne sich um seine Deckung zu kümmern.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <GarantierterEffekt>
        <![CDATA[Der Charakter lößt sich aus dem Nahkampf.]]>
      </GarantierterEffekt>
  </Taktik>

  <Taktik Id="Finte" Kosten="1" Belastung="1" Typ="Offensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Finte]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Der Charakter versucht mit dem Angriff den gegner eine Falle zu stellen und so stärker zu bedrängen.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <SofortigerEffekt>
        <![CDATA[$x entspricht dem Bedränungsmalus des Ziels]]>
      </SofortigerEffekt>
    <Erfolg>
        <![CDATA[Das Ziel wird writer bedrängt anstelle das Schaden verursact wird.]]>
      </Erfolg>
    <Mod ModifierType="Malus">
      <VariableModValueType Value="X" />
    </Mod>
    <Eigenschaften>
      <Eigenschaft Id="Präzise" />
    </Eigenschaften>

  </Taktik>

  <Taktik Id="Wuchtiger Angriff" Kosten="3" Belastung="1" Typ="Offensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Wuchtiger Angriff]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Setzt mehr Wucht in einen auszuführenden Angriff.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <SofortigerEffekt>
        <![CDATA[$x ist  belibiger Wert bis zum doppelten Basiswuchtschaden der Waffe.]]>
      </SofortigerEffekt>
    <Erfolg>
        <![CDATA[Die Waffe Trift. Erhöht den Wuchtschaden der Waffe um $x.]]>
      </Erfolg>
    <Misserfolg>
        <![CDATA[Erhalte Positions Mali in höhe von $x.]]>
      </Misserfolg>
    <Mod ModifierType="Malus">
      <MultiplyModValueType>
        <ConcreteModValueType Value="2" Type="Absolute" />
        <VariableModValueType Value="X" />
      </MultiplyModValueType>
    </Mod>
    <Eigenschaften>
      <Eigenschaft Id="Wuchtig" />
    </Eigenschaften>
  </Taktik>

  <Taktik Id="Todesstoß" Kosten="5" Belastung="2" Typ="Offensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Todesstoß]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Der Charakter leitet einen Riskanten Angriff ein um den gegner einen Gefährlichen Treffer zu versetzen.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <SofortigerEffekt>
        <![CDATA[$x ist ein belibiger Wert.]]>
      </SofortigerEffekt>
    <GarantierterEffekt>
        <![CDATA[Die Defenskiv Taktik des Ziels wird um $x reduziert.]]>
      </GarantierterEffekt>
    <Erfolg>
        <![CDATA[Wenn der Defensivtaktikwert des Ziels unter 0 sinkt, verursacht die Waffe Schaden.]]>
      </Erfolg>
    <Misserfolg>
        <![CDATA[Erhalte Positions Mali in höhe von 3$x.]]>
      </Misserfolg>
    <Mod ModifierType="Malus">
      <MultiplyModValueType>
        <ConcreteModValueType Value="3" Type="Absolute" />
        <VariableModValueType Value="X" />
      </MultiplyModValueType>
    </Mod>
  </Taktik>

  <Taktik Id="Waffe Platzieren" Kosten="2" Belastung="2" Typ="Offensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Waffe Platzieren]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Eine Taktik an deren ende die Waffe an eine empfindliche stelle, wie beispielsweise Kehle, Plaziert wurde.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <Bedingung>
        <![CDATA[Pro Waffe kann nur eine Aktion Waffe Platzieren gewählt werden.]]>
      </Bedingung>
    <SofortigerEffekt>
        <![CDATA[$x ist ein belibiger Wert.]]>
      </SofortigerEffekt>
    <Erfolg>
        <![CDATA[Führt das Ziel nächste Runde eine Offensive Taktik aus erhält es den Schnittschaden der Waffe. Sollte die Taktik nur eine Defensive sein, so wird der Schaden nur erlitten falls diese ein Misserfolg ist.

Zusätzlich erhällt das Ziel nächste runde einen zusätzlichen Malus von $x auf alle Offensiven Taktiken und der Charakter einen Bonus von $x auf alle Defensiven Taktiken.

Ausserdem darf eine Waffe Plazieren Taktik die das selbe Ziel wie diese hat in der Nächsten runde durch eine andere Taktik ausgetauscht werden, nachdem alle Aktionen bekannt gegeben wurden. Als Trefferzone kann die selebe Zone gewählt werden die diese Aktion getroffen hat. In diesem Fall kann eine belibige Unterzone gewählt werden.]]>
      </Erfolg>
    <Mod ModifierType="Malus">
      <AddModValueType>
        <VariableModValueType Value="X" />
        <ConcreteModValueType Type="Absolute" Value="5" />
      </AddModValueType>
    </Mod>
  </Taktik>

  <Taktik Id="Schutz" Kosten="3" Belastung="1" Typ="Defensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Schutz]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Der Charakter übernimmt den Schutz eines anderen.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <Bedingung>
        <![CDATA[Das Ziel darf keine Offensive Taktik nutzen.]]>
      </Bedingung>
    <SofortigerEffekt>
        <![CDATA[Diese Taktik gillt als Defensive Aktion des zu schützenden Ziels und nicht für den ausführenden Charakter.]]>
      </SofortigerEffekt>
  </Taktik>

  <Taktik Id="Position" Kosten="2" Belastung="1" Typ="Defensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Position]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Der Charakter versucht seine Position zu verbessern.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <SofortigerEffekt>
        <![CDATA[Diese Defensive Taktik führt nicht zum Misserfolg einer gengerichen Offensiven Taktik und verhindert keinen Schaden.]]>
      </SofortigerEffekt>
    <Erfolg>
        <![CDATA[Der Charakter baut die hälfte seiner Stellenmali ab (aufgerundet).]]>
      </Erfolg>
    <Misserfolg>
        <![CDATA[Der Charakter baut ein virtel seiner Stellenmali ab (aufgerundet). Würde der Charakter diese runde Stellen Mali bekommen, entfellt der Effekt dafür wird der erhaltene Mali um die hälfte Reduziert.]]>
      </Misserfolg>
  </Taktik>

  <Taktik Id="Reden" Kosten="1" Belastung="0" Typ="Neutral">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Reden]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Der Charakter kann am ende der Kampfrunde mehr sagen als er eigentlich dürfte. Er kann einen ungefähr 5 sekunden lange nachricht von sich geben.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <SofortigerEffekt>
        <![CDATA[Der Charakter erhält einen Malus von 1 auf alle Aktionen. Wenn dies die einzige Aktion ist, werden die Kosten auf 0 reduziert.]]>
      </SofortigerEffekt>
  </Taktik>

  <Taktik Id="Zustechen" Kosten="2" Belastung="0.5" Typ="Unterstützend">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Zustechen]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Bei diesem Angriff nutzt der Charakter die Schneide seiner Waffe an einer äußerst empfindlichen stelle, wie beispielsweise dem Hals. Oder Durschtößt mit der Klinge den Körper des Gegners. Ein bevorzugter Angriff von meuchlern.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <Erfolg>
        <![CDATA[Der Wuchtschaden der Waffe wird auf 0 gesetzt (Wuchtschläge funktionieren). Der Angriff erhält die Eigenschaft Tödlich.]]>
      </Erfolg>
    <Mod ModifierType="Malus">
      <ConcreteModValueType Type="Percent" Value="50" />
    </Mod>

  </Taktik>

  <Taktik Id="Gezielter Angriff" Kosten="1" Belastung="0" Typ="Unterstützend">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Gezielter Angriff]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Der Kämpfer versucht ein bestimmtes Körperteil gezielt zu treffen.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <SofortigerEffekt>
        <![CDATA[$x ist ien belibiger Wert.]]>
      </SofortigerEffekt>
    <Erfolg>
        <![CDATA[Es werden $x mehr Trefferzonen bestimmt. Wähle eine dieser.]]>
      </Erfolg>
    <Mod ModifierType="Malus">
      <MultiplyModValueType>
        <VariableModValueType Value="X" />
        <VariableModValueType Value="X" />
      </MultiplyModValueType>
    </Mod>
    <Eigenschaften>
      <Eigenschaft Id="Präzise" />
    </Eigenschaften>
  </Taktik>

  <Taktik Id="Präziser Angriff" Kosten="1" Belastung="0" Typ="Unterstützend">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Präzieser Angriff]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Erlaubt dem Angreifer an der Rüstung des Gegners Vorbei zu stechen..]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <Bedingung>
        <![CDATA[Nur mit Präzisen Waffen möglich.]]>
      </Bedingung>
    <SofortigerEffekt>
        <![CDATA[$x ist ien belibiger Wert.]]>
      </SofortigerEffekt>
    <Erfolg>
        <![CDATA[Bei genauer bestimmung der Trefferzone (W10) kann $x zum Ergebniss Addiert oder Subtrahiert werden.]]>
      </Erfolg>
    <Mod ModifierType="Malus">
      <MultiplyModValueType>
        <ConcreteModValueType Type="Absolute" Value="2" />
        <VariableModValueType Value="X" />
      </MultiplyModValueType>
    </Mod>
    <Eigenschaften>
      <Eigenschaft Id="Präzise" />
    </Eigenschaften>
  </Taktik>

  <Taktik Id="Erholen / Abwarten" Kosten="0" Belastung="1" Typ="Neutral">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Erholen / Abwarten]]>
      </misc:Lokalisirung>
    </misc:Name><misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Der Charakter erholt sich. Zusätzlich kann er sich ein besseren Überblick in Komplexen kampfgeschehen verschaffen.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <GarantierterEffekt>
        <![CDATA[Der Charakter regeneriert alle Erschöpfung, wenn dies seien einzigste Aktion ist. Andernfalls die Hälfte.]]>
      </GarantierterEffekt>
  </Taktik>

  <Taktik Id="Zielen" Kosten="1" Belastung="2" Typ="Neutral">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Zielen]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Der Charakter bereitet einen gezielten Schuss vor.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <Bedingung>
        <![CDATA[Nur mit Fernkampfwaffe]]>
      </Bedingung>
    <SofortigerEffekt>
        <![CDATA[$x entspricht der Fernkampfmodifikation. (Siehe Tabelle)]]>
      </SofortigerEffekt>
    <Erfolg>
        <![CDATA[Der Charakter erhält einen Bonus in Höhe der halben Punkte auf seine nächste Taktik falls diese das selbe Ziel hat und mit der selben Waffe erfolt.]]>
      </Erfolg>
    <Mod ModifierType="Malus">
      <MultiplyModValueType>
        <VariableModValueType Value="X" />
        <ConcreteModValueType Type="Percent" Value="50" />
      </MultiplyModValueType>
    </Mod>
  </Taktik>

  <Taktik Id="Gezielter Schuss" Kosten="1" Belastung="2" Typ="Offensiv">
    <misc:Name>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Gezielter Schuss]]>
      </misc:Lokalisirung>
    </misc:Name>
    <misc:Beschreibung>
      <misc:Lokalisirung Sprache="de">
        <![CDATA[Der Charakter führt einen gezielten Schuss durch.]]>
      </misc:Lokalisirung>
    </misc:Beschreibung>
    <SofortigerEffekt>
        <![CDATA[$x entspricht der Fernkampf modifikation.]]>
      </SofortigerEffekt>
    <Erfolg>
        <![CDATA[Der Charakter Trifft das Ziel einmal.]]>
      </Erfolg>
    <Mod ModifierType="Malus">
      <SubstractModValueType>
        <VariableModValueType Value="X" />
        <ConcreteModValueType Value="3" Type="Absolute" />
      </SubstractModValueType>
    </Mod>
  </Taktik>

