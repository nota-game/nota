        <lebewesen:Berechnung id="gewicht" Reihenfolge="2" einheit="kg" gruppe="abgeleitet">
            <misc:Name>
                <misc:Lokalisirung Sprache="de">Gewicht</misc:Lokalisirung>
            </misc:Name>
            <lebewesen:Formel>bmi*(größe/100)*(größe/100)</lebewesen:Formel>
        </lebewesen:Berechnung>
        <lebewesen:Berechnung id="ausdauer" Reihenfolge="10" gruppe="abgeleitet">
            <misc:Name>
                <misc:Lokalisirung Sprache="de">Ausdaure</misc:Lokalisirung>
            </misc:Name>
            <lebewesen:Formel>70-(KO+KO+ST)</lebewesen:Formel>
        </lebewesen:Berechnung>
        <lebewesen:Berechnung id="kraftGewichtCap" Reihenfolge="10">
            <misc:Name>
                <misc:Lokalisirung Sprache="de">Maximales Gewicht für Kraft</misc:Lokalisirung>
            </misc:Name>
            <lebewesen:Formel>9999999999</lebewesen:Formel>
        </lebewesen:Berechnung>
        <lebewesen:Berechnung id="kraft" Reihenfolge="10" gruppe="abgeleitet">
            <misc:Name>
                <misc:Lokalisirung Sprache="de">Kraft</misc:Lokalisirung>
            </misc:Name>
            <lebewesen:Formel>_over = max(0,gewicht-kraftGewichtCap);_under =
                min(kraftGewichtCap,gewicht);ceil((91/30 * (1/ ST )*(1/ ST
                )+1529/300*1/ST+3/50)*_under)</lebewesen:Formel>
        </lebewesen:Berechnung>
        <lebewesen:Berechnung id="wiederstandsbonus" Reihenfolge="10" gruppe="abgeleitet">
            <misc:Name>
                <misc:Lokalisirung Sprache="de">Wiederstandsbonus</misc:Lokalisirung>
            </misc:Name>
            <lebewesen:Formel>floor((21-KO)/3)</lebewesen:Formel>
        </lebewesen:Berechnung>
        
